  <script type="text/javascript">
      $.fn.modal.Constructor.prototype.enforceFocus = function() {};
  </script>

  <div class="form-group singleton">
    <div style="float:left">
      <%= render partial: 'partials/label_with_help',
                 locals: {f: f,
                          label_val: 'Version Release Identifier',
                          include_help: 'Example: "3.1"'} %>
    </div>
    <div style="clear:both"></div>
    <div>
      <%= f.input :release_identifier, required: true, label: false, class: 'form-control' %>
    </div>
  </div>

  <div class="form-group singleton">
    <div style="float:left">
      <%= render partial: 'partials/label_with_help',
                 locals: {f: f,
                          label_val: 'Release Date',
                          include_help: 'Example: "December 2022"'} %>
    </div>
    <div style="clear:both"></div>
    <div>
      <%= f.input :release_date, required: true, label: false, class: 'form-control' %>
    </div>
  </div>

    <br /><br />